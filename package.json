{
  "name": "tutors",
  "repository": "https://github.com/tutors-sdk/tutors.git",
  "author": "Eamonn De Leastar <eamonn.deleastar@setu.ie>",
  "license": "MIT",
  "version": "0.3.0",
  "private": true,
  "workspaces": [
    "/*",
    "cli/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:app": "turbo run build --filter tutors-app",
    "preview:app": "turbo run preview --filter tutors-app",
    "test": "turbo run test",
    "test:app": "turbo run test --filter tutors-app",
    "dev": "turbo run dev --parallel",
    "dev:app": "turbo run dev --filter tutors-app",
    "lint": "turbo run lint",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "eslint-config-custom": "*",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "turbo": "1.10.13"
  },
  "packageManager": "npm@8.19.2"
}
